{

  "description": "Adds a button to view and edit cookies.",
  "manifest_version": 2,
  "name": "C00kie Monster",
  "version": "1.01",
  "homepage_url": "https://github.com/dylanmm/c00kie-monster",
  "icons": {
    "48": "icons/cookie_icon.png"
  },

  "applications": {
    "gecko": {
      "id": "C00kieMonster-extension@example.com",
      "strict_min_version": "46.0a1"
    }
  },

  "permissions": [
    "<all_urls>","tabs","cookies" 
  ],

  "browser_action": {
    "default_icon": "icons/cookie_icon.png",
    "default_title": "Choose a user agent",
    "default_popup": "popup/c00kie.html"
  }

}
